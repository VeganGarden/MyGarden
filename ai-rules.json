{
  "aiServices": {
    "chat": {
      "name": "AI聊天服务",
      "description": "提供智能对话功能",
      "endpoint": "/ai-service/chat",
      "methods": ["POST"],
      "parameters": {
        "message": "string",
        "context": "array"
      }
    },
    "analyze": {
      "name": "数据分析服务",
      "description": "提供数据分析和洞察功能",
      "endpoint": "/ai-service/analyze",
      "methods": ["POST"],
      "parameters": {
        "data": "object",
        "analysisType": "string"
      }
    }
  },
  "permissions": {
    "read": ["ai-rules", "functions"],
    "write": ["functions"],
    "execute": ["ai-service"]
  },
  "configurations": {
    "timeout": 30,
    "memory": 128,
    "environment": "production"
  }
}